package training;

import java.util.*;

public class ChkUnion {
	public static void main(String args[]) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[n]; 
		
		for(int i=0; i<n; i++) {
			a[i]=sc.nextInt();
		}
		
		int m=sc.nextInt();
		int b[]=new int[m];
				
		for(int i=0; i<m; i++) {
			b[i]=sc.nextInt();
		}
		
		HashSet<Integer> c = new HashSet<>();
		for(int i:a)
			c.add(i);
		for(int i:b)
			c.add(i);
		
		System.out.println(c.size());
		
	}
}
