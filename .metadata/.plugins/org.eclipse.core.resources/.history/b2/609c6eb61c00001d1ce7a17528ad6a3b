package training;

import java.util.Arrays;
import java.util.Scanner;

public class ChkUnion {
	public static void main(String args[]) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[n]; 
				
		for(int i=0; i<n; i++) {
			a[i]=sc.nextInt();
		}
		
		int m=sc.nextInt();
		int b[]=new int[m];
				
		for(int i=0; i<m; i++) {
			b[i]=sc.nextInt();
		}
		
		Arrays.sort(a);
		Arrays.sort(b);
		
		int l=n+m;
		int c[]=new int[l];
		
		for(int i=0; i<n; i++) {
			c[i]=a[i];
		}
		
		/*for(int i=0; i<n; i++) {
			if()
		}*/
		
		
		for(int i=0; i<l; i++) {
			System.out.print(c[i]+" ");
		}
	}

}
